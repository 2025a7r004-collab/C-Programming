#include<stdio.h>
int main(){
int x;
printf("Enter the value of x ");
scanf("%d", &x );
(x%2==0)?printf("Number is Even") : printf("Number is Odd");
return 0;
}


#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int N;
    scanf("%d", &N);
    if(N%2==0){
        printf("Even");
    }
    else{
        printf("Odd");
    }
    return 0;
}
